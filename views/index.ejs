<%- include('./partials/head.ejs') %>

<header></header>

<main>
  <section class="left-side-information">
    <table border="1" cellspacing="0" cellpadding="0">
      <thead>
        <tr>
          <th>Dorp</th>
          <th>Accommodatie</th>
          <th>Kamertype</th>
        </tr>
      </thead>
      <tbody>
        <% const firstId = [...new Set(data.map(dataload => dataload.accomodationId))];
        firstId.forEach(id => { const rows = data.filter(dataload =>
        dataload.accomodationId === id); if (rows.length > 0) { %>
        <tr>
          <td></td>
          <td><%= rows[0].complex_name %></td>
          <td><%= rows[0].accomodatie_description %></td>
        </tr>
        <% } }); %>
      </tbody>
    </table>
    <table border="1" cellspacing="0" cellpadding="0">
      <thead>
        <tr>
          <th>23-12-2023</th>
          <th>30-12-2023</th>
          <th>06-01-2024</th>
          <th>13-01-2024</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
        </tr>
      </tbody>
    </table>
  </section>
</main>

