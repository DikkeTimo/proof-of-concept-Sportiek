<%- include('./partials/head.ejs') %>

<header></header>

<main>
  <section class="information">
    <table id="left" border="1" cellspacing="0" cellpadding="0">
      <thead>
        <tr>
          <th>Accommodatie</th>
          <th>Kamertype</th>
          <th>Personen</th>
          <th>Datum</th>
        </tr>
      </thead>
      <tbody>
        <% checkdata.forEach(info => { %>
        <tr>
          <td><%= info.complex_name %></td>
          <td><%= info.accomodatie_description %></td>
          <td><%= info.departurePricePersons %></td>
          <td><%= info.departureDate %></td>
      </tbody>
      <% }) %>
    </table>
  </section>
</main>

<%- include('./partials/foot.ejs') %>
